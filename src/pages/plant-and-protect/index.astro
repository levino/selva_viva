---
import { getCollection, getEntry } from "astro:content";
import PlantAndProtectIndex from "../../pageLayouts/PlantAndProtectIndex.astro";

const plantAndProtectIndexContent = await getEntry(
  "plantAndProtectIndexPage",
  "en"
);

const plantAndProtect = await getCollection("plantAndProtect", ({ id }) => {
  return id.startsWith("en/");
});
---

<PlantAndProtectIndex
  indexContent={plantAndProtectIndexContent.data}
  plantAndProtect={plantAndProtect}
/>
