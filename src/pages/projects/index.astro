---
import ProjectsPage from "../../pageLayouts/ProjectsPage.astro";
import { getEntry } from "astro:content";
const treeNursery = await getEntry("treeNursery", "en");
const theFinca = await getEntry("theFinca", "en");
const pageContentDe = await getEntry("projectsPage", "en");

const projects = [treeNursery, theFinca];
---

<ProjectsPage projects={projects} pageContent={pageContentDe} />
