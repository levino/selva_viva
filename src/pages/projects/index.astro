---
import CenterContentVideoBg from "../../components/CenterContentVideoBg.astro";
import ProjectsPage from "../../pageLayouts/ProjectsPage.astro";
import * as pageContent from "../../content/_projects_index.md";

import { getCollection } from "astro:content";
const allProjects = await getCollection("projects");
const [natureReserve] = await getCollection("natureReserve");
const projects = {
  projects: allProjects,
  natureReserve,
};
---

<ProjectsPage projects={projects} pageContent={pageContent.frontmatter} />
